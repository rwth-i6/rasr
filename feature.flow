<?xml version="1.0" ?>
<network name="network">
  <out name="features"/>
  <param name="id"/>
  <param name="track"/>
  <node block-size="400" filter="stream-input" name="audio" sample-rate="16000" sample-type="s16"/>
  <node filter="generic-vector-s16-demultiplex" name="demultiplex" track="$(track)"/>
  <link from="audio" to="demultiplex"/>
  <node filter="generic-convert-vector-s16-to-vector-f32" name="convert"/>
  <link from="demultiplex" to="convert"/>
  <node filter="generic-vector-f32-multiplication" name="scale" value="3.0517578125e-05"/>
  <link from="convert" to="scale"/>
  <link from="scale" to="network:features"/>
</network>
